<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        style="?atrkkrBasicInfoTextStyle"
        android:id="@+id/text_event_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/activity_margin"
        android:layout_marginTop="@dimen/activity_margin"
        app:layout_constraintVertical_chainStyle="packed"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/divider_event_title"
        tools:text="Sire Name"/>
    <com.weyr_associates.animaltrakkerfarmmobile.app.core.widget.Divider
        android:id="@+id/divider_event_title"
        android:layout_width="0dp"
        android:layout_height="@dimen/divider_thickness"
        android:layout_marginHorizontal="@dimen/activity_margin"
        android:layout_marginTop="@dimen/interior_spacing_margin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_event_title"
        app:layout_constraintBottom_toTopOf="@id/text_date_time_in"/>
    <TextView
        style="?atrkkrBasicInfoLabelStyle"
        android:id="@+id/text_date_time_in_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/activity_margin"
        android:text="@string/text_date_time_in"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/text_date_time_in"
        app:layout_constraintTop_toTopOf="@id/text_date_time_in"/>
    <TextView
        style="?atrkkrBasicInfoLabelStyle"
        android:id="@+id/text_date_time_in"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/activity_margin"
        android:layout_marginTop="@dimen/interior_spacing_margin"
        app:layout_constraintStart_toEndOf="@id/text_date_time_in_label"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider_event_title"
        app:layout_constraintBottom_toTopOf="@id/text_date_time_out"
        tools:text="Date/Time In"/>
    <TextView
        style="?atrkkrBasicInfoLabelStyle"
        android:id="@+id/text_date_time_out_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/activity_margin"
        android:text="@string/text_date_time_out"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/text_date_time_out"
        app:layout_constraintTop_toTopOf="@id/text_date_time_out"/>
    <TextView
        style="?atrkkrBasicInfoLabelStyle"
        android:id="@+id/text_date_time_out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/activity_margin"
        android:layout_marginTop="@dimen/interior_spacing_margin"
        app:layout_constraintStart_toEndOf="@id/text_date_time_out_label"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_date_time_in"
        app:layout_constraintBottom_toTopOf="@id/text_service_type"
        tools:text="Date/Time Out"/>
    <TextView
        style="?atrkkrBasicInfoLabelStyle"
        android:id="@+id/text_service_type_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/activity_margin"
        android:text="@string/text_service_type"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/text_service_type"
        app:layout_constraintTop_toTopOf="@id/text_service_type"/>
    <TextView
        style="?atrkkrBasicInfoLabelStyle"
        android:id="@+id/text_service_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/activity_margin"
        android:layout_marginTop="@dimen/interior_spacing_margin"
        app:layout_constraintStart_toEndOf="@id/text_service_type_label"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_date_time_out"
        app:layout_constraintBottom_toTopOf="@id/text_date_time_birthing"
        tools:text="Service Type"/>
    <TextView
        style="?atrkkrBasicInfoLabelStyle"
        android:id="@+id/text_date_time_birthing_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/activity_margin"
        android:text="@string/text_birthing_date_time"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/text_date_time_birthing"
        app:layout_constraintTop_toTopOf="@id/text_date_time_birthing"/>
    <TextView
        style="?atrkkrBasicInfoLabelStyle"
        android:id="@+id/text_date_time_birthing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/activity_margin"
        android:layout_marginTop="@dimen/interior_spacing_margin"
        app:layout_constraintStart_toEndOf="@id/text_date_time_birthing_label"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_service_type"
        app:layout_constraintBottom_toTopOf="@id/divider_event_info"
        tools:text="Date/Time Birthing"/>
    <androidx.constraintlayout.widget.Group
        android:id="@+id/group_male_event_info"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:constraint_referenced_ids="text_date_time_in_label,text_date_time_in,text_date_time_out_label,text_date_time_out,text_service_type_label,text_service_type,text_date_time_birthing_label,text_date_time_birthing,divider_event_info"
        tools:visibility="visible"/>
    <androidx.constraintlayout.widget.Group
        android:id="@+id/group_birthing_date_time"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:constraint_referenced_ids="text_date_time_birthing_label,text_date_time_birthing"/>
    <com.weyr_associates.animaltrakkerfarmmobile.app.core.widget.Divider
        android:id="@+id/divider_event_info"
        android:layout_width="0dp"
        android:layout_height="@dimen/divider_thickness"
        android:layout_marginHorizontal="@dimen/activity_margin"
        android:layout_marginTop="@dimen/interior_spacing_margin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_date_time_birthing"
        app:layout_constraintBottom_toTopOf="@id/text_birth_notes"/>
    <TextView
        style="?atrkkrBasicInfoLabelStyle"
        android:id="@+id/text_birth_notes"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/activity_margin"
        android:layout_marginVertical="@dimen/interior_spacing_margin"
        android:singleLine="false"
        android:maxLines="10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider_event_info"
        app:layout_constraintBottom_toTopOf="@id/divider_birth_notes"
        tools:text="Birthing Notes"/>
    <com.weyr_associates.animaltrakkerfarmmobile.app.core.widget.Divider
        android:id="@+id/divider_birth_notes"
        android:layout_width="0dp"
        android:layout_height="@dimen/divider_thickness"
        android:layout_marginHorizontal="@dimen/activity_margin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_birth_notes"
        app:layout_constraintBottom_toTopOf="@id/barrier_totals_top"/>
    <androidx.constraintlayout.widget.Group
        android:id="@+id/group_birth_notes"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:constraint_referenced_ids="text_birth_notes,divider_birth_notes"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier_totals_top"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:barrierDirection="top"
        app:constraint_referenced_ids="container_birth_totals_by_sex,text_no_offspring"/>
    <LinearLayout
        android:id="@+id/container_birth_totals_by_sex"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginBottom="@dimen/activity_margin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider_birth_notes"
        app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
        style="?atrkkrBasicInfoTextStyle"
        android:id="@+id/text_no_offspring"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/activity_margin"
        android:layout_marginTop="@dimen/interior_spacing_margin"
        android:layout_marginBottom="@dimen/activity_margin"
        android:text="@string/text_no_offspring"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider_birth_notes"
        app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
